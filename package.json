{
  "name": "keystrokemanager",
  "displayName": "KeystrokeManager",
  "description": "Constantly checking keystrokes while programming.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keystrokemanager.keystrokeCountAnalytics",
        "title": "Keystrokes: Show Count-Analytics"
      },
      {
        "command": "keystrokemanager.mostOftenPressedKeys",
        "title": "Keystrokes: Show most often pressed keys"
      },
      {
        "command": "keystrokemanager.savingToConfiguration",
        "title": "Keystrokes: saving"
      },
      {
        "command": "keystrokemanager.loadingFromConfiguration",
        "title": "Keystrokes: loading"
      }
    ],
    "configuration": [
      {
        "title": "KeystrokeManager",
        "properties": {
          "keystrokeManager.keystrokes": {
            "type": "object",
            "default": {
              "total": "0",
              "year": "0",
              "month": "0",
              "week": "0",
              "day": "0",
              "hour": "0",
              "minute": "0",
              "second": "0"
            },
            "description": "Collection of all keystrokes in different timestamps."
          } 
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "moment": "^2.29.4"
  }
}
